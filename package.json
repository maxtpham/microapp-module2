{
  "name": "@microapp/module1",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "files": [
    "bin/prd",
    "*.sh"
  ],
  "scripts": {
    "build:docker:dev": "sh -c \". ../../subscription/common.subscription/docker/.build/.env-dev.sh && ../../subscription/common.subscription/docker/.build/build.sh\"",
    "build:docker:prd": "sh -c \". ../../subscription/common.subscription/docker/.build/.env-prd.sh && ../../subscription/common.subscription/docker/.build/build.sh\"",
    "k8s:create": "sh -c \". ../../subscription/common.subscription/docker/.k8s/.env.sh && ../../subscription/common.subscription/docker/.k8s/create.sh\"",
    "k8s:delete": "sh -c \". ../../subscription/common.subscription/docker/.k8s/.env.sh && ../../subscription/common.subscription/docker/.k8s/delete.sh\"",
    "k8s:apply": "sh -c \". ../../subscription/common.subscription/docker/.k8s/.env.sh && ../../subscription/common.subscription/docker/.k8s/apply.sh\"",
    "build": "if-env NODE_ENV=production && npm run build:prd || npm run build:dev",
    "b": "npm run build",
    "build:dev": "cross-env-shell NODE_ENV=development \"rm -rf bin/dev && cp -R ./base/html/dev ./bin/dev && npm run build:cmd\"",
    "build:prd": "cross-env-shell NODE_ENV=production \"rm -rf bin/prd && cp -R ./base/html/prd ./bin/prd && npm run build:cmd\"",
    "build:cmd": "npm install && node base/scripts/common/version.js && webpack",
    "start": "if-env NODE_ENV=production && npm run start:prd || npm run start:dev",
    "start:dev:docker": "cross-env-shell NODE_ENV=development DEV_ENV=docker \"npm run build && webpack-dev-server --public admin.mybestbook.dev --port 80 --host 0.0.0.0 --hot --inline\"",
    "start:dev": "cross-env-shell NODE_ENV=development DEV_PORT=3002 \"webpack-dev-server --port 3002 --hot --inline --progress --colors --open-page bin/dev/\"",
    "start:prd": "cross-env-shell NODE_ENV=production \"npm run build:prd && start http://localhost:3002 && http-server ./bin/prd/ -p 3002\""
  },
  "keywords": [],
  "author": "thanhptr",
